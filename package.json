{
  "name": "serverless-secure",
  "version": "1.0.1",
  "description": "Lambda Secure Serverless Services",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm i",
    "test": "jest --coverage",
    "build": "tsc --project tsconfig.json",
    "build:package": "npx -p typescript@3.8.3 tsc --project tsconfig.json",
    "lint": "tslint --project ./tsconfig.json 'src/**/*.{ts,tsx}'",
    "deploy:local": "npm run build && npm link",
    "develop": "tsc-watch --project tsconfig.json",
    "postinstall": "npm run build:package"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/serverless": "^1.72.5",
    "aws-lambda": "^1.0.6",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "node-yaml": "^4.0.1",
    "request": "^2.88.2",
    "source-map-support": "^0.5.10",
    "stringify-object": "^3.3.0",
    "unzip-stream": "^0.3.0",
    "update-notifier": "^4.1.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/fs-extra": "^9.0.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.3",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.159",
    "@types/node": "^13.13.13",
    "@types/request": "^2.48.5",
    "@types/update-notifier": "^4.1.1",
    "aws-sdk": "^2.710.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "readme-md-generator": "^1.0.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.6",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "files": [
    "src",
    "package.json",
    "tsconfig.json"
  ],
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmullings/serverless-secure.git"
  },
  "keywords": [
    "Lambda",
    "Secure",
    "Serverless",
    "Services"
  ],
  "author": "Jason Mullings <admin@serverless-secure.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmullings/serverless-secure/issues"
  },
  "homepage": "https://github.com/jmullings/serverless-secure#readme"
}
