{
  "name": "serverless-secure",
  "version": "1.0.3",
  "description": "Lambda  Serverless Security Session",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm run test",
    "build": "cp ./.env.$(git rev-parse --symbolic-full-name --abbrev-ref HEAD) .env && rimraf dist && tsc --project tsconfig.json --declaration",
    "build:package": "cp ./.env.$(git rev-parse --symbolic-full-name --abbrev-ref HEAD) .env && npx -p typescript@3.8.3 tsc --project tsconfig.json",
    "pretest": "npm run lint",
    "test": "cp ./.env.$(git rev-parse --symbolic-full-name --abbrev-ref HEAD) .env && jest --coverage",
    "test:snyk": "snyk test",
    "test:sonarQube": "node sonarqube.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "develop": "rimraf dist && tsc-watch --project tsconfig.json --onSuccess \"node dist/index.js\" ",
    "deploy": "npm run build",
    "deploy:compodoc": "compodoc -p tsconfig.json"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.1",
    "fs-extra": "^9.1.0",
    "iconv-lite": "^0.6.2",
    "jszip": "^3.5.0",
    "lodash": "^4.17.20",
    "require-from-string": "^2.0.2",
    "source-map-support": "^0.5.10",
    "stringify-object": "^3.3.0",
    "ts-morph": "^8.2.0",
    "update-notifier": "^5.0.1",
    "yawn-yaml": "^1.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@hapi/joi": "^17.1.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/fs-extra": "^9.0.6",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.3",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.168",
    "@types/node": "^13.13.40",
    "@types/request": "^2.48.5",
    "@types/serverless": "^1.78.20",
    "@types/stringify-object": "^3.3.0",
    "@types/update-notifier": "^4.1.1",
    "aws-sdk": "^2.831.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^25.5.4",
    "rimraf": "^3.0.2",
    "serverless-webpack": "^5.2.0",
    "snyk": "^1.439.1",
    "sonarqube-scanner": "^2.8.0",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.9.6",
    "webpack": "^4.46.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "files": [
    "dist/**/*",
    "package.json",
    "package-lock.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serverless-secure/serverless-secure.git"
  },
  "keywords": [
    "Lambda",
    "Secure",
    "Serverless",
    "Services"
  ],
  "author": "Serverless-Secure <admin@serverless-secure.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serverless-secure/serverless-secure/issues"
  },
  "homepage": "https://github.com/serverless-secure/serverless-secure#readme",
  "directories": {
    "test": "test"
  }
}
