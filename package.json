{
  "name": "serverless-secure",
  "version": "1.0.2",
  "description": "Lambda Secure Serverless Services",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prepublish": "npm run build",
    "precommit": "npm run test",
    "build": "rimraf dist && tsc --project tsconfig.json --declaration",
    "build:package": "npx -p typescript@3.8.3 tsc --project tsconfig.json",
    "pretest": "npm run lint",
    "test": "snyk test && jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "develop": "rimraf dist && tsc-watch --project tsconfig.json --onSuccess \"node dist/index.js\" ",
    "deploy": "npm run deploy:local && npm run deploy:git",
    "deploy:local": "npm run build && npm run deploy:compodoc && git add .",
    "deploy:compodoc": "compodoc -p tsconfig.json",
    "deploy:git": "git rm -r --cached src/* && git commit -am 'master dist set.' && git push private master"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.1",
    "fs-extra": "^9.0.1",
    "iconv-lite": "^0.6.2",
    "jszip": "^3.5.0",
    "lodash": "^4.17.20",
    "require-from-string": "^2.0.2",
    "source-map-support": "^0.5.10",
    "stringify-object": "^3.3.0",
    "ts-morph": "^8.1.0",
    "update-notifier": "^5.0.1",
    "yawn-yaml": "^1.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@hapi/joi": "^17.1.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/fs-extra": "^9.0.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.2.3",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.159",
    "@types/node": "^13.13.13",
    "@types/request": "^2.48.5",
    "@types/serverless": "^1.72.5",
    "@types/stringify-object": "^3.3.0",
    "@types/update-notifier": "^4.1.1",
    "aws-sdk": "^2.710.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "rimraf": "^3.0.2",
    "serverless-webpack": "^5.2.0",
    "snyk": "^1.388.0",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.9.6",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "files": [
    "dist/**/*",
    "package.json",
    "package-lock.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "snyk test",
      "pre-push": "npm test "
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serverless-secure/serverless-secure.git"
  },
  "keywords": [
    "Lambda",
    "Secure",
    "Serverless",
    "Services"
  ],
  "author": "Serverless-Secure <admin@serverless-secure.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serverless-secure/serverless-secure/issues"
  },
  "homepage": "https://github.com/serverless-secure/serverless-secure#readme",
  "directories": {
    "test": "test"
  }
}
